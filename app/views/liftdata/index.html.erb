<p id="notice"><%= notice %></p>

<h1>Liftdata</h1>

<table>
  <thead>
    <tr>
      <th>Exercise</th>
      <th>Set</th>
      <th>Rep</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liftdata.each do |liftdatum| %>
      <tr>
        <td><%= liftdatum.exercise %></td>
        <td><%= liftdatum.set %></td>
        <td><%= liftdatum.rep %></td>
        <td><%= liftdatum.weight %></td>
        <td><%= link_to 'Show', liftdatum %></td>
        <td><%= link_to 'Edit', edit_liftdatum_path(liftdatum) %></td>
        <td><%= link_to 'Destroy', liftdatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Liftdatum', new_liftdatum_path %>
